{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.hbs",
      "use": "@vercel/static"
    },
    {
      "src": "news.css",
      "use": "@vercel/static"
    },
    {
      "src": "news.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.hbs"
    }
  ]
}
